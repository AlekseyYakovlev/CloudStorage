<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="db.SqliteMasterEntity" table="sqlite_master" schema="main">
        <property name="type">
            <column name="type" sql-type="text" not-null="true"/>
        </property>
        <property name="name">
            <column name="name" sql-type="text" not-null="true"/>
        </property>
        <property name="tblName">
            <column name="tbl_name" sql-type="text" not-null="true"/>
        </property>
        <property name="rootpage">
            <column name="rootpage" sql-type="integer" not-null="true"/>
        </property>
        <property name="sql">
            <column name="sql" sql-type="text" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>